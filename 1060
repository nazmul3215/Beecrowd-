#include<stdio.h>
int main()
{
    double a,b,d,e,f,g;
    scanf("%lf%lf%lf%lf%lf%lf",&a,&b,&d,&e,&f,&g);
    int c = 0;
    if (a > 0)
    {
        c++;
    }
        if (b>0)
        {
            c++;
        }
        if (d>0)
        {
            c++;
        }
        if (e>0)
        {
            c++;
        }
        if (f > 0)
        {
            c++;
        }
        if (g > 0)
        {
            c++;
            printf("%d valores positivos\n", c);
        }
    return 0;
}
